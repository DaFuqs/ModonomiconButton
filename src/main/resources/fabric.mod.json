{
  "schemaVersion": 1,
  "id": "modonomiconbutton",
  "version": "${version}",
  "name": "ModonomiconButton",
  "description": "This is an addon for the mod Modonomicon which adds a button in the inventory screen",
  "authors": [
    "Globox_Z",
    "DaFuqs"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/modonomiconbutton",
    "sources": "https://github.com/Globox1997/ModonomiconButton",
    "issues": "https://github.com/Globox1997/ModonomiconButton/issues"
  },
  "license": "MIT",
  "icon": "assets/modonomiconbutton/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.modonomiconbutton.ModonomiconButtonMain"
    ],
    "client": [
      "net.modonomiconbutton.ModonomiconButtonClient"
    ],
    "modmenu": [
      "net.modonomiconbutton.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "modonomiconbutton.mixins.json"
  ],
  "depends": {
    "java": ">=17",
    "minecraft": ">=${minecraft_version}",
    "fabric-api": ">=${fabric_version}",
    "fabricloader": ">=${loader_version}",
    "cloth-config2": ">=${cloth_config_version}",
    "modonomicon": "*"
  }
}